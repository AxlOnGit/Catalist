                  FROM artikellieferanten
                  WHERE (FesterLieferant_Kz = '*')
                  GROUP BY Artikel) l ON a.Artikel = l.Artikel
WHERE 
 (k.Kundennummer = @Kunde) AND (a.USER_Katalogartikel = '1') AND (a.Statuskennzeichen &lt;&gt; 'Ja') AND (a.Sperrvermerk1 = 0)

UNION

