                    <CommandText>SELECT 
	m.UID, 
	o.Modellbezeichnung AS Maschine, 
	(CASE 
		When Seriennummer Is Null Then ''
		Else Seriennummer
	End) As Seriennummer,
	x.Zuordnungsbeginn AS Zuordnung, 
	k.Kundennummer,
	LEFT(k.Kundennummer, 5) AS Kunde, 
	k.Name1 AS Firma, 
	k.Postleitzahl,
	k.Ort,
	s.Wartungskennzeichen,
