INNER JOIN cpm_maschinenserie s ON o.MaschinenserieId = s.UID
INNER JOIN kunden k ON x.Kundennummer = k.Kundennummer
WHERE        
	(x.Zuordnungsende &gt; NOW())
ORDER BY 
	Maschine</CommandText>
