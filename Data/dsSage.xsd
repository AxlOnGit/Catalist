GROUP BY 
	o.Kontonummer, o.Belegnummer, o.Belegdatum
HAVING 
	(SUM(o.Rechnungsbetr_InFiBu_Waehrung) - SUM(o.ZahlbetragInFiBu_Waehrung) &gt; 0)
