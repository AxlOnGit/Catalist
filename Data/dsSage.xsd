                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT 
	o.Kontonummer, 
  k.Name1 as Firma, 
  o.Belegnummer, 
  o.Belegdatum, 
  o.Nettozahlungsziel,
	SUM(o.Rechnungsbetr_InFiBu_Waehrung) AS Rechnungsbetrag, 
  SUM(o.ZahlbetragInFiBu_Waehrung) AS Gezahlt, 
  SUM(o.Rechnungsbetr_InFiBu_Waehrung) - SUM(o.ZahlbetragInFiBu_Waehrung) AS Offen,
  (TO_DAYS(CURDATE() + o.Nettozahlungsziel)) - (TO_DAYS(o.Belegdatum)) AS Tage
