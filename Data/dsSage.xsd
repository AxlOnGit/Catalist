                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT 
  Kontonummer, 
  SUM(Rechnungsbetr_InFiBu_Waehrung) AS Rechnungsbetrag, 
  SUM(ZahlbetragInFiBu_Waehrung) AS Zahlbetrag, 
  SUM(Rechnungsbetr_InFiBu_Waehrung) - SUM(ZahlbetragInFiBu_Waehrung) AS OPBetrag
